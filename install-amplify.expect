#!/usr/bin/expect

# One hour is probably long enough to finish the build - way overkill for just the input part of the script though
set timeout 3600

spawn /install-source.sh

expect "Enter your API key: "
send "<CHANGEAPIKEY>\r"
expect "Please select your OS family:"
send "3\r"
expect "Continue (y/n)?"
send "y\r"

expect eof
catch wait result
